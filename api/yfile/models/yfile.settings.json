{
  "kind": "collectionType",
  "connection": "default",
  "collectionName": "yfiles",
  "info": {
    "name": "yfile"
  },
  "options": {
    "increments": true,
    "timestamps": true
  },
  "attributes": {
    "codigo": {
      "type": "string",
      "unique": true,
      "required": true
    },
    "descripcion": {
      "type": "richtext"
    },
    "servicio_generals": {
      "collection": "servicio-general"
    },
    "biblia": {
      "model": "biblia"
    },
    "cliente": {
      "model": "cliente"
    },
    "operador_mayorista": {
      "model": "operador-mayorista"
    },
    "operador_minorista": {
      "model": "operador-minorista"
    },
    "servicio_hospedajes": {
      "collection": "servicio-hospedaje"
    },
    "servicio_transportes": {
      "collection": "servicio-transporte"
    }
  }
}
