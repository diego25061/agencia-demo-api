{
  "kind": "collectionType",
  "connection": "default",
  "collectionName": "yfiles",
  "info": {
    "name": "yfile"
  },
  "options": {
    "increments": true,
    "timestamps": true
  },
  "attributes": {
    "codigo": {
      "type": "string",
      "unique": true,
      "required": true
    },
    "descripcion": {
      "type": "richtext"
    },
    "biblia": {
      "model": "biblia"
    },
    "cliente": {
      "model": "cliente"
    },
    "servicios": {
      "collection": "servicio",
      "via": "yfile"
    }
  }
}
