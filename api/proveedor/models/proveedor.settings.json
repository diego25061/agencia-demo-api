    "nombre": {
      "type": "string",
      "required": true
    },
    "correo": {
      "type": "email",
      "unique": false
    },
    "correo2": {
      "type": "email"
    },
    "numero": {
      "type": "string"
    },
    "numero2": {
      "type": "string"
    },
    "ciudad": {
      "type": "string"
    },
    "clase": {
      "type": "enumeration",
      "enum": [
        "hotel",
        "restaurante",
        "transporte",
        "guia",
        "operador",
        "empresa",
        "persona"
      ]
    }
  }
}
